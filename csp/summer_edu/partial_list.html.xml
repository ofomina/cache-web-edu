<?xml version="1.0" encoding="UTF-8"?>
<Export generator="Cache" version="25">
<CSP name="partial_list.html" application="/csp/healthshare/summer_edu/" default="1"><![CDATA[
<h2>New Item:</h2>
id: <input type="text" ng-model="newItem.id">
name: <input type="text" ng-model="newItem.name">
<a href ng-click="addNewItem()">Add</a>

<h2>List:</h2>
<ul>
  <li ng-repeat="item in list">
    <a ui-sref="app.item({index:$index})">id: {{item.id}}, name: <b>{{item.name}}</b></a>
  </li>
</ul>
<span class="label label-default">{{appCtrl.info}}</span>]]></CSP>
</Export>
